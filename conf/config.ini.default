[LOG]
; ******log設定******
; 關閉log功能 輸入選項 (true, True, 1) 預設 不關閉
; LOG_DISABLE=1

; logs路徑 預設 logs
; LOG_PATH=

; 關閉紀錄log檔案 輸入選項 (true, True, 1)  預設 關閉
; LOG_FILE_DISABLE=1

; 設定紀錄log等級 DEBUG,INFO,WARNING,ERROR,CRITICAL 預設WARNING
; LOG_LEVEL=

[TELEGRAM]
; TELEGRAM_API_KEY=
; TELEGRAM_CHAT_ID=

[HOST_SETTING]
; 主機相關設定json檔路徑 預設 .conf/host.json
; HOST_JSON_PATH=
; 監控間隔時間 單位：秒 預設 10秒
; MONITORING_INTERVAL=